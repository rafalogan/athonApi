{
  "name": "athon_api",
  "version": "1.0.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "dev": "nodemon ./server.ts",
    "build": "rm -rf ./dist && tsc",
    "test": "npx jest --no-cahce --watchAll --config ./jest.config.ts",
    "test:cov": "npx jest --no-cahce --config ./jest.config.ts",
    "start": "pm2-runtime start ecosystem.config.yml",
    "start:database": "docker-compose down && docker-compose up",
    "stop:database": "docker-compose down",
    "flow": "git branch -l",
    "flow:all": "git branch -l -a",
    "flow:init": "git flow init",
    "flow:rm": "git branch -D",
    "feature:start": "git flow feature start",
    "feature:end": "git flow feature finish",
    "release:start": "VERSION=$(node -p \"require('./package').version\") && git flow release start $VERSION",
    "release:start:m": "git flow release start",
    "release:end": "BRANCH=$(node -p \"require('./package').version\") && git flow release finish $BRANCH",
    "hotfix:start": "git flow hotfix start",
    "hotfix:end": "git flow hotfix finish",
    "support:start": "git flow support start",
    "support:end": "git flow support finish",
    "add:patch": "npm version patch && git push",
    "add:minor": "npm version minor && git push",
    "add:major": "npm version major && git push",
    "git:c": "git add -A && git status && git commit -am",
    "up:branch": "BRANCH=$(git branch --show-current) && git push --set-upstream origin $BRANCH",
    "up:commit": "git pull && git push",
    "knex:migrate:make": "knex --knexfile src/knexfile.ts -x ts migrate:make",
    "knex:migrate:latest": "knex --knexfile src/knexfile.ts migrate:latest",
    "knex:migrate:rollback": "knex --knexfile src/knexfile.ts migrate:rollback"
  },
  "keywords": [
    "API",
    "TypeScript",
    "NodeJS",
    "Postgres",
    "MongoDB"
  ],
  "author": "RafaDraw - rafadraw@gmail.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rafalogan/athonApi"
  },
  "homepage": "https://github.com/rafalogan/athonApi#readme",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http": "*",
    "http-status": "^1.5.0",
    "https": "^1.0.0",
    "is-empty": "^1.2.0",
    "jwt-simple": "^0.5.6",
    "knex": "^0.95.4",
    "md5": "^2.3.0",
    "mongodb": "^3.6.5",
    "mongoose": "^5.12.3",
    "morgan": "^1.10.0",
    "nodemailer": "^6.5.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.5.1",
    "pm2": "^4.5.6",
    "redis": "^3.1.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.1",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.10",
    "@types/eslint": "^7.2.9",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/express": "^4.17.11",
    "@types/is-empty": "^1.2.0",
    "@types/jwt-simple": "^0.5.33",
    "@types/md5": "^2.3.0",
    "@types/mongodb": "^3.6.12",
    "@types/mongoose": "^5.10.4",
    "@types/morgan": "^1.9.2",
    "@types/nodemailer": "^6.4.1",
    "@types/passport": "^1.0.6",
    "@types/passport-jwt": "^3.0.5",
    "@types/pg": "^7.14.11",
    "@types/redis": "^2.8.28",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "faker": "^5.5.3",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "sucrase": "^3.18.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "eslintConfig": {
    "plugins": [
      "@typescript-eslint/eslint-plugin",
      "@typescript-eslint/parser",
      "@types/eslint-plugin-prettier"
    ],
    "env": {
      "node": true
    }
  }
}
