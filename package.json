{
  "name": "athon_api",
  "version": "2.1.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "lint": "npx eslint . --ext .ts",
    "lint:fix": "npx eslint . --ext .ts --fix",
    "style": "prettier --check 'src/**/*.ts' 'test/**/*ts'",
    "style:fix": "prettier --write 'src/**/*.ts' 'test/**/*ts'",
    "dev": "nodemon ./index.ts",
    "build": "rm -rf ./dist && tsc",
    "test": "npx jest --no-cahce --runInBand --watchAll --config ./jest.config.ts",
    "test:func": "NODE_ENV=test npx jest --no-cahce --runInBand --config ./test/jest.config.ts",
    "test:cov": "NODE_ENV=test npx ts-node test/jest-startup.ts",
    "start": "npx pm2-runtime start ecosystem.config.yml",
    "stop": "npx pm2 kill",
    "start:database": "docker-compose down && docker-compose up",
    "stop:database": "docker-compose down",
    "flow": "git branch -l",
    "flow:all": "git branch -l -a",
    "flow:init": "git flow init",
    "flow:rm": "git branch -D",
    "feature:start": "git flow feature start",
    "feature:finish": "git flow feature finish",
    "release:start": "VERSION=$(node -p \"require('./package').version\") && git flow release start $VERSION",
    "release:start:m": "git flow release start",
    "release:finish": "BRANCH=$(node -p \"require('./package').version\") && git flow release finish $BRANCH",
    "hotfix:start": "git flow hotfix start",
    "hotfix:finish": "git flow hotfix finish",
    "support:start": "git flow support start",
    "support:finish": "git flow support finish",
    "add:patch": "npm version patch && git push",
    "add:minor": "npm version minor && git push",
    "add:major": "npm version major && git push",
    "git:c": "git add -A && git status && git commit -am",
    "up:branch": "BRANCH=$(git branch --show-current) && git push --set-upstream origin $BRANCH",
    "up:commit": "git pull && git push",
    "up:tags": "git push --follow-tags",
    "knex:migrate:make": "npx knex migrate:make --knexfile knexfile.ts -x ts",
    "knex:migrate:latest": "npx knex migrate:latest --knexfile knexfile.ts",
    "knex:migrate:rollback": "npx knex migrate:rollback --knexfile knexfile.ts",
    "athon:create:config": "zsh ./bin/create.config.sh",
    "athon:create:entity": "zsh ./bin/create.entity.sh",
    "athon:create:module": "zsh ./bin/create.module.sh",
    "athon:create:service": "zsh ./bin/create.service.sh",
    "athon:create:model": "zsh ./bin/create.model.sh",
    "athon:create:type": "zsh ./bin/create.type.sh"
  },
  "keywords": [
    "API",
    "TypeScript",
    "NodeJS",
    "Postgres",
    "Redis"
  ],
  "author": "RafaDraw - rafadraw@gmail.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rafalogan/athonApi"
  },
  "homepage": "https://github.com/rafalogan/athonApi#readme",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.1125.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.1",
    "express": "^4.18.1",
    "http": "^0.0.1-security",
    "http-status": "^1.5.1",
    "https": "^1.0.0",
    "is-empty": "^1.2.0",
    "jwt-simple": "^0.5.6",
    "knex": "^1.0.7",
    "md5": "^2.3.0",
    "module-alias": "^2.2.2",
    "mongodb": "^4.5.0",
    "mongoose": "^6.4.6",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "multer-s3": "^2.10.0",
    "nodemailer": "^6.7.4",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.3",
    "pm2": "^5.2.0",
    "redis": "^4.0.6",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/is-empty": "^1.2.1",
    "@types/jest": "^27.4.1",
    "@types/jwt-simple": "^0.5.33",
    "@types/md5": "^2.3.2",
    "@types/module-alias": "^2.0.1",
    "@types/mongodb": "^4.0.7",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/multer-s3": "^2.7.12",
    "@types/node": "^17.0.31",
    "@types/nodemailer": "^6.4.4",
    "@types/passport": "^1.0.7",
    "@types/passport-jwt": "^3.0.6",
    "@types/pg": "^8.6.5",
    "@types/redis": "^4.0.11",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "@vscode/sqlite3": "^5.0.8",
    "dotenv": "^16.0.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "faker": "^5.5.3",
    "jest": "^27.5.1",
    "jest-cli": "^28.0.3",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "source-map": "^0.7.3",
    "sqlite3": "^5.0.5",
    "sucrase": "^3.21.0",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.6.3"
  },
  "_moduleAliases": {
    "@root": ".",
    "@deep": "src/some/very/deep/directory/or/file",
    "@my_module": "lib/some-file.js",
    "something": "src/foo"
  }
}
