{
  "name": "athon_api",
  "version": "1.11.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "lint": "npx eslint . --ext .ts",
    "lint:fix": "npx eslint . --ext .ts --fix",
    "style": "prettier --check 'src/**/*.ts' 'test/**/*ts'",
    "style:fix": "prettier --write 'src/**/*.ts' 'test/**/*ts'",
    "dev": "nodemon ./src/server.ts",
    "build": "rm -rf ./dist && tsc",
    "test": "npx jest --no-cahce --runInBand --watchAll --config ./jest.config.ts",
    "test:func": "NODE_ENV=test npx jest --no-cahce --runInBand --config ./test/jest.config.ts",
    "test:cov": "NODE_ENV=test npx jest --no-cahce --config ./jest.config.ts --coverage",
    "start": "pm2-runtime start ecosystem.config.yml",
    "start:database": "docker-compose down && docker-compose up",
    "stop:database": "docker-compose down",
    "flow": "git branch -l",
    "flow:all": "git branch -l -a",
    "flow:init": "git flow init",
    "flow:rm": "git branch -D",
    "feature:start": "git flow feature start",
    "feature:finish": "git flow feature finish",
    "release:start": "VERSION=$(node -p \"require('./package').version\") && git flow release start $VERSION",
    "release:start:m": "git flow release start",
    "release:finish": "BRANCH=$(node -p \"require('./package').version\") && git flow release finish $BRANCH",
    "hotfix:start": "git flow hotfix start",
    "hotfix:finish": "git flow hotfix finish",
    "support:start": "git flow support start",
    "support:finish": "git flow support finish",
    "add:patch": "npm version patch && git push",
    "add:minor": "npm version minor && git push",
    "add:major": "npm version major && git push",
    "git:c": "git add -A && git status && git commit -am",
    "up:branch": "BRANCH=$(git branch --show-current) && git push --set-upstream origin $BRANCH",
    "up:commit": "git pull && git push",
    "up:tags": "git push --follow-tags",
    "knex:migrate:make": "npx knex migrate:make --knexfile knexfile.ts -x ts",
    "knex:migrate:latest": "npx knex migrate:latest --knexfile knexfile.ts",
    "knex:migrate:rollback": "npx knex migrate:rollback --knexfile knexfile.ts"
  },
  "keywords": [
    "API",
    "TypeScript",
    "NodeJS",
    "Postgres",
    "MongoDB"
  ],
  "author": "RafaDraw - rafadraw@gmail.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rafalogan/athonApi"
  },
  "homepage": "https://github.com/rafalogan/athonApi#readme",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "http": "*",
    "http-status": "^1.5.0",
    "https": "^1.0.0",
    "is-empty": "^1.2.0",
    "jwt-simple": "^0.5.6",
    "knex": "^0.95.4",
    "md5": "^2.3.0",
    "module-alias": "^2.2.2",
    "moment": "^2.29.1",
    "mongodb": "^3.6.5",
    "mongoose": "^5.12.7",
    "morgan": "^1.10.0",
    "nodemailer": "^6.6.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.6.0",
    "pm2": "^4.5.6",
    "redis": "^3.1.2",
    "ts-node": "^9.1.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.1",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/is-empty": "^1.2.0",
    "@types/jest": "^26.0.23",
    "@types/jwt-simple": "^0.5.33",
    "@types/md5": "^2.3.0",
    "@types/module-alias": "^2.0.0",
    "@types/mongodb": "^3.6.12",
    "@types/mongoose": "^5.10.5",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.44",
    "@types/nodemailer": "^6.4.1",
    "@types/passport": "^1.0.6",
    "@types/passport-jwt": "^3.0.5",
    "@types/pg": "^7.14.11",
    "@types/redis": "^2.8.28",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "dotenv": "^8.6.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "faker": "^5.5.3",
    "jest": "^26.5.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "sqlite3": "^5.0.2",
    "sucrase": "^3.18.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.6",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "typescript": "^4.2.4"
  }
}
